{
  "name": "eloquent-ffmpeg",
  "version": "0.1.0-alpha.0",
  "description": "An easy to use API for FFmpeg interactions",
  "module": "lib/lib.es6.js",
  "main": "lib/lib.cjs",
  "types": "lib/lib.d.ts",
  "scripts": {
    "build": "rollup -c",
    "types": "ts-node -r dotenv/config scripts/ffmpeg_types.ts",
    "pretest": "ts-node -r dotenv/config scripts/test_samples.ts",
    "test": "mocha -r dotenv/config -r ts-node/register test/**/*_test.ts",
    "coverage": "nyc --reporter=lcov npm test",
    "typedoc": "typedoc && node -e \"require('fs').writeFileSync('docs/.nojekyll', '')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FedericoCarboni/eloquent-ffmpeg.git"
  },
  "keywords": [],
  "author": {
    "name": "Federico Carboni",
    "email": "fr3ddydev@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/FedericoCarboni/eloquent-ffmpeg/issues"
  },
  "homepage": "https://github.com/FedericoCarboni/eloquent-ffmpeg#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rollup": "^2.28.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "uuid": "^8.3.0"
  }
}
